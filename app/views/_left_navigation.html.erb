<paper-item nav-item
            icon="store"
            label="Store">
  <%= link_to '', root_path %>
</paper-item>
<% if user_signed_in? %>
  <paper-item nav-item
              icon="social:people"
              label="Manage Users">
    <%= link_to '', admin_users_path %>
  </paper-item>
  <paper-item nav-item
              icon="lock"
              label="Sign out">
    <%= link_to '', destroy_user_session_path, method: :delete, data: { confirm: 'Are you sure?' } %>
  </paper-item>
<% else %>
  <paper-item nav-item
              icon="lock"
              label="Sign in / Register">
    <%= link_to '', new_user_session_path %>
  </paper-item>
<% end %>
